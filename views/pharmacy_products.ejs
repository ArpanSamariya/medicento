        <script>
            window.onload = function () {
	
    var chart = new CanvasJS.Chart("chartContainer", {
        animationEnabled: true,
        title: {
            text: "Medicine And Quantity"
        },
        axisX: {
            interval: 1,
            title: "Medicine"
        },
        axisY: {
            title: "Oty"
        },
        data: [{
            type: "bar",
            dataPoints: [
                { label: "E", y: 5 },
                { label: "D", y: 5 },
                { label: "C", y: 5 },
                { label: "B", y: 11 },
                { label: "A", y: 20 }
            ]
        }]
    });
    chart.render();
    
    var chart2 = new CanvasJS.Chart("chartContainer1", {
        animationEnabled: true,
        title: {
            text: "Manufacturer And S K U's"
        },
        axisX: {
            interval: 1,
            title: "Manufacturer"
        },
        axisY: {
            title: "S K U's"
        },
        data: [{
            type: "bar",
            dataPoints: [
                { label: "A Pharma", y: 5 },
                { label: "D Pharma", y: 2 },
                { label: "C Pharma", y: 10 },
                { label: "B Pharma", y: 15 },
                { label: "Bio Pharma", y: 20 }
            ]
        }]
    });
    chart2.render();
    
    }
        </script>
