<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags always come first -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Distributor Dashboard</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="css/pharma.css">
    <!-- Bootstrap core CSS -->
    <link href="css/style.min.css" rel="stylesheet">
    <script>
        window.onload = function () {    
                var options = {
                    animationEnabled: true,
                    theme: "light2",
                    title: {
                        text: "Monthly Sales Data"
                    },
                    axisX: {
                        valueFormatString: "MMM"
                    },
                    axisY: {
                        prefix: "Rs.",
                        labelFormatter: addSymbols
                    },
                    toolTip: {
                        shared: true
                    },
                    legend: {
                        cursor: "pointer",
                        itemclick: toggleDataSeries
                    },
                    data: [
                        {
                            type: "column",
                            name: "Actual Sales",
                            showInLegend: true,
                            xValueFormatString: "MMMM YYYY",
                            yValueFormatString: "Rs#,##0",
                            dataPoints: [
                                { x: new Date(2018, 0), y: 20000 },
                                { x: new Date(2018, 1), y: 25000 },
                                { x: new Date(2018, 2), y: 30000 },
                                { x: new Date(2018, 3), y: 70000, indexLabel: "High Renewals" },
                                { x: new Date(2018, 4), y: 40000 },
                                { x: new Date(2018, 5), y: 60000 },
                                { x: new Date(2018, 6), y: 55000 },
                                { x: new Date(2018, 7), y: 33000 },
                                { x: new Date(2018, 8), y: 45000 }
                            ]
                        },
                        {
                            type: "line",
                            name: "Expected Sales",
                            showInLegend: true,
                            yValueFormatString: "Rs#,##0",
                            dataPoints: [
                                { x: new Date(2018, 0), y: 32000 },
                                { x: new Date(2018, 1), y: 37000 },
                                { x: new Date(2018, 2), y: 40000 },
                                { x: new Date(2018, 3), y: 52000 },
                                { x: new Date(2018, 4), y: 45000 },
                                { x: new Date(2018, 5), y: 47000 },
                                { x: new Date(2018, 6), y: 42000 },
                                { x: new Date(2018, 7), y: 43000 },
                                { x: new Date(2018, 8), y: 41000 }
                            ]
                        },
                        {
                            type: "area",
                            name: "Profit",
                            markerBorderColor: "white",
                            markerBorderThickness: 2,
                            showInLegend: true,
                            yValueFormatString: "Rs#,##0",
                            dataPoints: [
                                { x: new Date(2018, 0), y: 4000 },
                                { x: new Date(2018, 1), y: 7000 },
                                { x: new Date(2018, 2), y: 12000 },
                                { x: new Date(2018, 3), y: 40000 },
                                { x: new Date(2018, 4), y: 20000 },
                                { x: new Date(2018, 5), y: 35000 },
                                { x: new Date(2018, 6), y: 33000 },
                                { x: new Date(2018, 7), y: 20000 },
                                { x: new Date(2018, 8), y: 25000 }
                            ]
                        }]
                };
                $("#chartContainer").CanvasJSChart(options);
                function addSymbols(e) {
                    var suffixes = ["", "K", "M", "B"];
                    var order = Math.max(Math.floor(Math.log(e.value) / Math.log(1000)), 0);
                
                    if (order > suffixes.length - 1)
                        order = suffixes.length - 1;
                
                    var suffix = suffixes[order];
                    return CanvasJS.formatNumber(e.value / Math.pow(1000, order)) + suffix;
                }
                function toggleDataSeries(e) {
                    if (typeof (e.dataSeries.visible) === "undefined" || e.dataSeries.visible) {
                        e.dataSeries.visible = false;
                    } else {
                        e.dataSeries.visible = true;
                    }
                    e.chart.render();
                }
                
                var options1 = {
	                exportEnabled: true,
	                animationEnabled: true,
	                title:{
		                text: "Accounting"
	                },
	                legend:{
		                horizontalAlign: "right",
		                verticalAlign: "center"
	                },
	                data: [{
		                type: "pie",
		                showInLegend: true,
		                toolTipContent: "<b>{name}</b>: &#x20b9;{y} (#percent%)",
		                indexLabel: "{name}",
		                legendText: "{name} (#percent%)",
		                indexLabelPlacement: "inside",
		                dataPoints: [
			                { y: 6566.4, name: "Now - New Customers" },
			                { y: 7600.2, name: "Returning - Repeat Customers" }
		                ]
	                }]
                };
                $("#chartContainer1").CanvasJSChart(options1);

            }
        </script>
</head>

<body>
    <nav class="navbar navbar-inverse no-margin navbar-fixed-top">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header fixed-brand">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" id="menu-toggle">
                <span class="glyphicon glyphicon-th-large" aria-hidden="true"></span>
            </button>
            <a class="navbar-brand" href="/distributor"><i class="fa fa-user"></i> Medicento_Distributor_Test</a>
        </div>
        <!-- navbar-header-->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav navbar-right">
                <li><a href="/distributor"><span class="glyphicon glyphicon-home"></span></a></li>
                <li><a href="/distributor_logout"><span class="glyphicon glyphicon-log-out"></span>Logout</a></li>
                <li><a href=""></a></li>
            </ul>
        </div>
        <!-- bs-example-navbar-collapse-1 -->
    </nav>
    <div id="wrapper">
        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav nav-pills nav-stacked" id="menu">
                <% if(title == 'Dashboard') {  %>
                <li class="active">
                    <% } else {%>
                <li>
                    <% } %>
                    <a href="/distributor_static"><span class="fa-stack fa-lg pull-left"><i class="fa fa-dashboard fa-stack-1x"></i></span>
                        Dashboard</a>
                </li>
                <% if(title == 'Orders') {  %>
                <li class="active">
                    <% } else {%>
                <li>
                    <% } %>
                    <a href="/distributor_order_static"><span class="fa-stack fa-lg pull-left"><i class="fa fa-cart-arrow-down fa-stack-1x "></i></span>Orders</a>
                </li>
                <% if(title == 'Retailers') {  %>
                <li class="active">
                    <% } else {%>
                <li>
                    <% } %>
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-users fa-stack-1x "></i></span>Retailers</a>
                </li>
                <% if(title == 'Inventoy Product') {  %>
                <li class="active">
                    <% } else {%>
                <li>
                    <% } %>
                    <a href="/distributor_product_static"> <span class="fa-stack fa-lg pull-left"><i class="fa fa-product-hunt fa-stack-1x "></i></span>Products</a>
                </li>
                <% if(title == 'Billing') {  %>
                <li class="active">
                    <% } else {%>
                <li>
                    <% } %>
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-fax fa-stack-1x "></i></span>Billing</a>
                </li>
                <% if(title == 'Accounting') {  %>
                <li class="active">
                    <% } else {%>
                <li>
                    <% } %>
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-dollar fa-stack-1x "></i></span>Accounting</a>
                </li>
            </ul>
        </div>
        <!-- /#sidebar-wrapper -->
        <div id="content">
            <div class="container">
                <h2 class="text-danger">
                    Medicento_Distributor_Test
                </h2>
                <h4>Today:
                    <%= moment(date).format('DD/MM/YY') %>
                </h4>
                <div style="background: rgb(34,34,34);height: 2px;margin-bottom: 10px;"></div>
                <div class="row">
                    <div class="col-xs-12 col-md-6 col-lg-4" style="height: 240px;">
                        <div class="panel panel-warning">
                            <div class="panel-heading">
                                <span style="margin-left:30%;">38 Order(s)</span>
                            </div>
                            <div class="panel-body">
                                <span>Approved 8</span>
                                <div class="progress" style="margin-bottom: -1px;">
                                    <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="50"
                                        aria-valuemax="100" aria-valuemin="0" style="width: 50%"></div>
                                </div>
                                <span>Packed 2</span>
                                <div class="progress" style="margin-bottom: -1px">
                                    <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="12.5"
                                        aria-valuemax="100" aria-valuemin="0" style="width: 12.5%"></div>
                                </div>
                                <span>Shipped 6</span>
                                <div class="progress" style="margin-bottom: -1px">
                                    <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="37.5"
                                        aria-valuemax="100" aria-valuemin="0" style="width: 37.5%"></div>
                                </div>
                                <span>Delivered 6</span>
                                <div class="progress" style="margin-bottom: -1px">
                                    <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="37.5"
                                        aria-valuemax="100" aria-valuemin="0" style="width: 37.5%"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-2"></div>
                    <div class="col-xs-12 col-md-6 col-lg-4" style="height: 240px;">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <span style="margin-left:30%;">40 Pharmacy</span>
                            </div>
                            <div class="panel-body">
                                <span>Vamsi </span>
                                <div class="progress" style="margin-bottom: -1px;">
                                    <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="50"
                                        aria-valuemax="100" aria-valuemin="0" style="width: 50%"></div>
                                </div>
                                <span>A.P</span>
                                <div class="progress" style="margin-bottom: -1px">
                                    <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="25"
                                        aria-valuemax="100" aria-valuemin="0" style="width: 10%"></div>
                                </div>
                                <span>Medishop</span>
                                <div class="progress" style="margin-bottom: -1px">
                                    <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="25"
                                        aria-valuemax="100" aria-valuemin="0" style="width: 20%"></div>
                                </div>
                                <span>Kiran</span>
                                <div class="progress" style="margin-bottom: -1px">
                                    <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="25"
                                        aria-valuemax="100" aria-valuemin="0" style="width: 20%"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row" style="margin-top: 20px;">
                    <div class="col-lg-3">
                        <div class="well">
                            <h4 class="text-danger">Max Order Size</h4>
                            <h4><span style="float: right;color: rgb(34,34,34);"><i class="fa fa-rupee"></i>40,000</span></h4>
                        </div>
                    </div>
                    <div class="col-lg-3">
                        <div class="well">
                            <h4 class="text-primary">Average Order Revenue</h4>
                            <h4><span style="float: right;color: rgb(34,34,34);"><i class="fa fa-rupee"></i>6,000</span></h4>
                        </div>
                    </div>
                    <div class="col-lg-3">
                        <div class="well">
                            <h4 class="text-primary">Total Revenue</h4>
                            <h4><span style="float: right;color: rgb(34,34,34);"><i class="fa fa-rupee"></i>3.4 Cr</span></h4>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-5">
                        <div id="chartContainer" style="height: 300px; width: 100%;"></div>
                    </div>
                    <div class="col-lg-5">
                        <div id="chartContainer1" style="height: 300px; width: 100%;"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- /#wrapper -->
    <script src="js/jquery-3.3.1.min.js"></script>
    <script src="js/pharma.js"></script>
    <script src="js/canvas.js"></script>
</body>

</html>