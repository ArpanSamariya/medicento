<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags always come first -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Distributor Dashboard</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="css/pharma.css">
    <!-- Bootstrap core CSS -->
    <link href="css/style.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-footable/3.1.6/footable.bootstrap.css">

</head>

<body>
    <nav class="navbar navbar-inverse no-margin navbar-fixed-top">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header fixed-brand">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" id="menu-toggle">
                <span class="glyphicon glyphicon-th-large" aria-hidden="true"></span>
            </button>
            <a class="navbar-brand" href="/distributor"><i class="fa fa-user"></i> TULSI PHARMA</a>
        </div>
        <!-- navbar-header-->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav navbar-right">
                <li><a href="/distributor"><span class="glyphicon glyphicon-home"></span></a></li>
                <li><a href="/distributor_logout"><span class="glyphicon glyphicon-log-out"></span>Logout</a></li>
                <li><a href=""></a></li>
            </ul>
        </div>
        <!-- bs-example-navbar-collapse-1 -->
    </nav>
    <div id="wrapper">
        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav nav-pills nav-stacked" id="menu">
                <% if(title == 'Dashboard') {  %>
                <li class="active">
                    <% } else {%>
                <li>
                    <% } %>
                    <a href="/distributor_tulsi"><span class="fa-stack fa-lg pull-left"><i class="fa fa-dashboard fa-stack-1x"></i></span>
                        Dashboard</a>
                </li>
                <% if(title == 'Orders') {  %>
                <li class="active">
                    <% } else {%>
                <li>
                    <% } %>
                    <a href="/distributor_order_tulsi"><span class="fa-stack fa-lg pull-left"><i class="fa fa-cart-arrow-down fa-stack-1x "></i></span>Orders</a>
                </li>
                <% if(title == 'Retailers') {  %>
                <li class="active">
                    <% } else {%>
                <li>
                    <% } %>
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-users fa-stack-1x "></i></span>Retailers</a>
                </li>
                <% if(title == 'Inventoy Product') {  %>
                <li class="active">
                    <% } else {%>
                <li>
                    <% } %>
                    <a href="/distributor_product_tulsi"> <span class="fa-stack fa-lg pull-left"><i class="fa fa-product-hunt fa-stack-1x "></i></span>Products</a>
                </li>
                <% if(title == 'Billing') {  %>
                <li class="active">
                    <% } else {%>
                <li>
                    <% } %>
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-fax fa-stack-1x "></i></span>Billing</a>
                </li>
                <% if(title == 'Accounting') {  %>
                <li class="active">
                    <% } else {%>
                <li>
                    <% } %>
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-dollar fa-stack-1x "></i></span>Accounting</a>
                </li>
            </ul>
        </div>
        <div id="content" style="margin-top: 30px;">
            <div class="container">
                <div class="row">
                    <div class="col-md-3">
                        <div class="panel panel-primary">
                            <div class="panel-heading">
                                <span style="margin-left:30%;">Total Items</span>
                            </div>
                            <div class="panel-body">
                                <span class="text-danger ">0</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="panel panel-danger">
                            <div class="panel-heading">
                                <span style="padding:10px;color: aliceblue;">Items LOW &darr; on stock</span>
                            </div>
                            <div class="panel-body">
                                <span class="text-danger ">0</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="panel panel-success">
                            <div class="panel-heading">
                                <span style="padding:10px;color: aliceblue;">Items EXC &uarr; on stock</span>
                            </div>
                            <div class="panel-body">
                                <span class="text-danger ">0</span>
                            </div>
                        </div>
                    </div>
                </div>
                <section id="upload-data-panel">
                    <form id="upload-form" action="/upload_tulsi" method="post" enctype="multipart/form-data">
                        <fieldset>
                            <legend>Upload Data1</legend>
                            <div>
                                <!-- The name here is important, and will be used later to reference the data -->
                                <input type="file" name="csvdata" accept=".csv" id="file">
                            </div><br>
                            <div>
                                <input id="submit" class="btn btn-primary" type="submit" value="Upload">
                            </div>
                        </fieldset>
                    </form>
                </section>
            </div>
            <% if(data == 'Helow') { %>
            <% } else { %>
            <div class="row" style="margin: 10px;">
                <div class="col-md-11">
                    <table id="porder" class="table table-striped " style="width:100%">
                        <thead>
                            <tr>
                                <% data.forEach(function(pro) { %>
                                <% if(pro != '') { %>
                                <th>
                                    <%= pro %>
                                </th>
                                <% } %>
                                <% })%>
                            </tr>
                            <thead>
                            <tbody>
                            </tbody>
                    </table>
                </div>
            </div>
            <% }%>
        </div>
    </div>
    </div>
    <!-- /#wrapper -->
    <script src="js/jquery-3.3.1.min.js"></script>
    <script src="js/pharma.js"></script>
    <script src="js/canvas.js"></script>
    <script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.19/js/dataTables.jqueryui.min.js"></script>
    <script>
        var rows = '';
        <% product1.forEach(function(pro) { %>
        var row = "<tr>";
        <% pro.forEach(function(pro1) { %>
        <% if(pro1 != '') { %>
        row += '<td><%= pro1 %></td>';
        <% } %>
        <% }) %>
        row += '</tr>';
        rows += row;
        <% }); %>
        $('tbody').append(rows);
        $('#porder').dataTable();
    </script>
</body>

</html>